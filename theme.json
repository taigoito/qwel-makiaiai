{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "dark",
					"name": "Dark",
					"color": "#273653"
				},
				{
					"slug": "medium-dark",
					"name": "Medium Dark",
					"color": "#486398"
				},
				{
					"slug": "medium-light",
					"name": "Medium Light",
					"color": "#999999"
				},
				{
					"slug": "light",
					"name": "Light",
					"color": "#d8d8d8"
				},
				{
					"slug": "primary",
					"name": "Primary",
					"color": "#ff661e"
				},
				{
					"slug": "primary-light",
					"name": "Primary Light",
					"color": "#ffceb7"
				}
			]
		},
		"layout": {
			"contentSize": "900px",
			"wideSize": "1200px"
		},
		"spacing": {
			"blockGap": null,
			"units": [
				"rem"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"slug": "sans-serif",
					"name": "Sans Serif",
					"fontFamily": "\"M PLUS Rounded 1c\", sans-serif"
				},
				{
					"slug": "system-font",
					"name": "System Font",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "var(--wp--custom--font-size--small)"
				},
				{
					"slug": "medium",
					"size": "var(--wp--custom--font-size--normal)"
				},
				{
					"slug": "large",
					"size": "var(--wp--custom--font-size--medium)"
				},
				{
					"slug": "x-large",
					"size": "clamp(1.5rem, 4vw, 3rem)"
				}
			],
			"dropCap": false
		},
		"custom": {
			"spacing": {
				"container-pad": "clamp(0.75rem, 2%, 1.125rem)",
				"container-pad-wide": "clamp(0.75rem, 2%, 1.5rem)",
				"x-small": "0.375rem",
				"small": "0.75rem",
				"medium": "1.5rem",
				"large": "3rem",
				"x-large": "6rem"
			},
			"font-size": {
				"small": "0.8rem",
				"normal": "1rem",
				"medium": "1.17rem",
				"large": "1.33rem",
				"x-large": "1.5rem"
			},
			"line-height": {
				"x-small": "1.2",
				"small": "1.44",
				"medium": "1.6",
				"large": "1.8",
				"x-large": "1.92"

			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"lineHeight": "var(--wp--custom--line-height--large)"
		},
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontWeight": "700",
					"fontSize": "var(--wp--custom--font-size--medium)",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--line-height--x-small)",
					"letterSpacing": "0.1em"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--custom--font-size--small)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--custom--font-size--small)"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "Arial, Helvetica, sans-serif",
					"fontWeight": "700",
					"fontSize": "var(--wp--custom--font-size--x-large)",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--line-height--x-small)",
					"letterSpacing": "0.1em"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "700",
					"fontSize": "var(--wp--custom--font-size--medium)",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--line-height--x-small)",
					"letterSpacing": "0.1em"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--custom--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--custom--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "700",
					"fontSize": "var(--wp--custom--font-size--normal)",
					"textTransform": "capitalize",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans-serif)",
					"fontWeight": "700",
					"fontSize": "var(--wp--custom--font-size--normal)",
					"textTransform": "capitalize",
					"lineHeight": "var(--wp--custom--line-height--x-small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--medium-dark)"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "g-nav",
			"title": "G Nav"
		},
		{
			"name": "section-intro",
			"title": "Section Intro"
		},
		{
			"name": "section-topics",
			"title": "Section Topics"
		},
		{
			"name": "section-firelog",
			"title": "Section Firelog"
		},
		{
			"name": "section-firemap",
			"title": "Section Firemap"
		},
		{
			"name": "section-about",
			"title": "Section About"
		},
		{
			"name": "section-access",
			"title": "Section Access"
		},
		{
			"name": "section-contact",
			"title": "Section Contact"
		},
		{
			"name": "sidebar",
			"title": "Sidebar"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
